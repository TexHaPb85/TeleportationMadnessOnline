<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MenuController">
    <children>
        <TableView fx:id="sessionTableView" layoutX="14.0" layoutY="14.0" prefHeight="339.0" prefWidth="280.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="34.0" text="id" />
                <TableColumn fx:id="p1Column" prefWidth="116.0" text="Player 1" />
                <TableColumn fx:id="p2Column" prefWidth="129.0" text="Player 2" />
            </columns>
        </TableView>
        <Button layoutX="331.0" layoutY="101.0" mnemonicParsing="false" onAction="#newGame" text="New Game" />
        <Button layoutX="332.0" layoutY="141.0" mnemonicParsing="false" onAction="#joinGame" text="Join Game" />
        <ImageView fx:id="userImageView" fitHeight="109.0" fitWidth="120.0" layoutX="497.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../../../MySnakesAndLaddersVersion/src/Sources/other/blue.png" />
            </image></ImageView>
        <Label fx:id="userInfoLabel" layoutX="485.0" layoutY="133.0" text="info:" />
      <Button layoutX="337.0" layoutY="188.0" mnemonicParsing="false" onAction="#refBtn" text="Refreash" />
    </children>
</AnchorPane>
